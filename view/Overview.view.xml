<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	controllerName="sap.training.exc17.fullscreen.view.Overview"
	xmlns:html="http://www.w3.org/1999/xhtml">
	
	<Page title="{i18n>overviewPageTitle}">
		<content>
			<Table items="{travel>/CarrierSet}" >
				<columns>
					<Column>
						<Text text="{i18n>columnId}"/>
					</Column>
					<Column>
						<Text text="{i18n>columnName}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem press="onPress" type="Navigation">
						<cells>
							<ObjectIdentifier title="{travel>Carrid}"/>
							<Text text="{travel>Carrname}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
	
</mvc:View>